{
  "npmScope": "nrwl-airlines",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "booking-app": {
      "tags": [
        "type:app",
        "scope:booking"
      ]
    },
    "booking-app-e2e": {
      "tags": [
        "type:e2e",
        "scope:booking"
      ],
      "implicitDependencies": [
        "booking-app"
      ]
    }
  }
}
