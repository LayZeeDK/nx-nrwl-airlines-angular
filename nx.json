{
  "npmScope": "nrwl-airlines",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "booking-desktop-app": {
      "tags": ["platform:desktop", "scope:booking", "type:app"]
    },
    "booking-desktop-app-e2e": {
      "tags": [],
      "implicitDependencies": ["booking-desktop-app"]
    },
    "booking-feature-shell": {
      "tags": ["scope:booking", "type:shell"]
    },
    "booking-data-access": {
      "tags": ["scope:booking", "type:data-access"]
    }
  }
}
