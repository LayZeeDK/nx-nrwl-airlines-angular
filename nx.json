{
  "npmScope": "nrwl-airlines",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "booking-desktop-app": {
      "tags": ["domain:booking", "platform:desktop", "type:app"],
      "implicitDependencies": ["shared-root-assets", "shared-root-styles"]
    },
    "booking-desktop-app-e2e": {
      "tags": ["domain:booking", "platform:desktop", "type:e2e"],
      "implicitDependencies": ["booking-desktop-app"]
    },
    "booking-data-access": {
      "tags": ["domain:booking", "scope:booking", "type:data-access"]
    },
    "shared-root-data-access": {
      "tags": ["domain:shared", "scope:root", "type:data-access"]
    },
    "shared-root-environments": {
      "tags": ["domain:shared", "scope:root", "type:environments"]
    },
    "booking-feature-passenger-info": {
      "tags": ["domain:booking", "scope:booking", "type:feature"]
    },
    "booking-feature-flight-search": {
      "tags": ["domain:booking", "scope:booking", "type:feature"]
    },
    "booking-mobile-app": {
      "tags": ["domain:booking", "platform:mobile", "type:app"],
      "implicitDependencies": ["shared-root-assets", "shared-root-styles"]
    },
    "booking-mobile-app-e2e": {
      "tags": [],
      "implicitDependencies": ["booking-mobile-app"]
    },
    "check-in-data-access": {
      "tags": ["domain:check-in", "scope:check-in", "type:data-access"]
    },
    "check-in-desktop-app": {
      "tags": ["domain:check-in", "platform:desktop", "type:app"],
      "implicitDependencies": ["shared-root-assets", "shared-root-styles"]
    },
    "check-in-desktop-app-e2e": {
      "tags": [],
      "implicitDependencies": ["check-in-desktop-app"]
    },
    "check-in-mobile-app": {
      "tags": ["domain:check-in", "platform:mobile", "type:app"],
      "implicitDependencies": ["shared-root-assets", "shared-root-styles"]
    },
    "check-in-mobile-app-e2e": {
      "tags": [],
      "implicitDependencies": ["check-in-mobile-app"]
    },
    "shared-ui-buttons": {
      "tags": ["domain:shared", "scope:shared", "type:ui"]
    },
    "shared-util-formatting": {
      "tags": ["domain:shared", "scope:shared", "type:util"]
    },
    "seatmap-feature-seat-listing": {
      "tags": ["domain:seatmap", "scope:seatmap", "type:feature"]
    },
    "seatmap-data-access": {
      "tags": ["domain:seatmap", "scope:seatmap", "type:data-access"]
    },
    "booking-root-feature-shell": {
      "tags": ["domain:booking", "scope:root", "type:shell"]
    },
    "check-in-root-feature-shell": {
      "tags": ["domain:check-in", "scope:root", "type:shell"]
    },
    "shared-root-assets": {
      "tags": ["domain:shared", "scope:root", "type:assets"]
    },
    "shared-root-styles": {
      "tags": ["domain:shared", "scope:root", "type:styles"]
    }
  }
}
