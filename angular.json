{
  "version": 1,
  "projects": {},
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "pnpm"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "linter": "eslint",
      "routing": false,
      "strict": true,
      "style": "css",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "strict": true,
      "style": "css",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:storybook-configuration": {
      "linter": "eslint"
    }
  }
}
