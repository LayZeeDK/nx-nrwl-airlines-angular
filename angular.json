{
  "version": 1,
  "projects": {
    "booking-desktop-app": {
      "projectType": "application",
      "root": "apps/booking/desktop-app",
      "sourceRoot": "apps/booking/desktop-app/src",
      "prefix": "booking",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/booking/desktop-app",
            "index": "apps/booking/desktop-app/src/index.html",
            "main": "apps/booking/desktop-app/src/main.ts",
            "polyfills": "apps/booking/desktop-app/src/polyfills.ts",
            "tsConfig": "apps/booking/desktop-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/booking/desktop-app/src/favicon.ico",
              "apps/booking/desktop-app/src/assets"
            ],
            "styles": ["apps/booking/desktop-app/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/booking/desktop-app/src/environments/environment.ts",
                  "with": "apps/booking/desktop-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "booking-desktop-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "booking-desktop-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "booking-desktop-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/booking/desktop-app/src/**/*.ts",
              "apps/booking/desktop-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/booking/desktop-app"],
          "options": {
            "jestConfig": "apps/booking/desktop-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "booking-desktop-app-e2e": {
      "root": "apps/booking/desktop-app-e2e",
      "sourceRoot": "apps/booking/desktop-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/booking/desktop-app-e2e/cypress.json",
            "tsConfig": "apps/booking/desktop-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "booking-desktop-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "booking-desktop-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/booking/desktop-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "booking-feature-shell": {
      "projectType": "library",
      "root": "libs/booking/feature-shell",
      "sourceRoot": "libs/booking/feature-shell/src",
      "prefix": "booking",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/booking/feature-shell/src/**/*.ts",
              "libs/booking/feature-shell/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/booking/feature-shell"],
          "options": {
            "jestConfig": "libs/booking/feature-shell/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "booking-data-access": {
      "projectType": "library",
      "root": "libs/booking/data-access",
      "sourceRoot": "libs/booking/data-access/src",
      "prefix": "nrwl-airlines",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/booking/data-access/src/**/*.ts",
              "libs/booking/data-access/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/booking/data-access"],
          "options": {
            "jestConfig": "libs/booking/data-access/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "pnpm"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "linter": "eslint",
      "routing": false,
      "strict": true,
      "style": "css",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "displayBlock": true,
      "skipTests": true,
      "style": "css"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "strict": true,
      "style": "css",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:storybook-configuration": {
      "linter": "eslint"
    }
  },
  "defaultProject": "booking-desktop-app"
}
